<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd">

  <context:annotation-config />

  <bean id="duke" class="com.learning.springIdol.PoeticJuggler">
  </bean>

  <bean id="sonnet29" class="com.learning.springIdol.Sonnet29" autowire-candidate="true"/>

  <bean id="stage" class="com.learning.springIdol.Stage"
        factory-method="getInstance"/>

  <bean id="auditorium" class="com.learning.springIdol.Auditorium"/>

  <bean id="saxophone" class="com.learning.springIdol.Saxophone" />

  <bean id="piano" class="com.learning.springIdol.Piano" />

  <bean id="carl" class="com.learning.springIdol.Instrumentalist">
    <property name="song" value="wild one"/>
  </bean>
</beans>